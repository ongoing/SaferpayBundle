{
    "name": "ongoing/saferpay-bundle",
    "description": "Symfony2 bundle for providing access to Saferpay. This is an extension to the JMSCorePaymentBundle.",
    "type": "symfony-bundle",
    "keywords": ["payment","saferpay"],
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Cludius",
            "email": "sven.cludius@valiton.com"
        },
        {
            "name": "Frederik Trostorf",
            "email": "frederik.trostorf@ongoing.ch"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1",
        "jms/payment-core-bundle": "~1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-0": { "Ongoing\\Payment\\SaferpayBundle": "" }
    },
    "target-dir": "Ongoing/Payment/SaferpayBundle"
}

